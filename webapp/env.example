# NeuraX Web App Environment Variables
# Copy this to .env and set your values

# Port for Flask server (Render sets this automatically)
PORT=5000

# Secret key for Flask session management
# Generate a random key: openssl rand -hex 32
SECRET_KEY=neurax_demo_key_change_in_production

# Optional: Flask debug mode (set to False in production)
FLASK_DEBUG=False

# Optional: Logging level
LOG_LEVEL=INFO

# Optional: Custom signaling server URL (if different from local)
# SIGNALING_SERVER_URL=http://localhost:5000

